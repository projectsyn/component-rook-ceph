{
  "extends": [
    "config:base",
    ":gitSignOff",
    ":disableDependencyDashboard"
  ],
  "postUpgradeTasks": {
    "commands": [
      "make gen-golden-all"
    ],
    "fileFilters": [ "tests/golden/**" ],
    "executionMode": "update"
  },
  "suppressNotifications": [ "artifactErrors" ],
  "labels": [
    "dependency"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["rook-ceph", "docker.io/rook/ceph"],
      "groupName": "rook"
    }
  ]
}
